{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "globals":{
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },    
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": [
            "error", 
            "LabeledStatement", 
            "WithStatement"]
    }
}
